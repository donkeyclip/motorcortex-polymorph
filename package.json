{
  "name": "@donkeyclip/motorcortex-polymorph",
  "version": "1.0.0",
  "description": "Your plugin description here",
  "main": "dist/motorcortex-polymorph.cjs.js",
  "module": "dist/motorcortex-polymorph.esm.js",
  "browser": "dist/motorcortex-polymorph.umd.js",
  "author": "",
  "repository": {
    "type": "git",
    "url": "Your git repo here"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "concurrently": "concurrently -c \"cyan.bold,magenta.bold\" --names \"JS,Styles\"",
    "lint:styles": "stylelint  --allow-empty-input \"src/**.css\" --config .stylelintrc.json",
    "lint:js": "eslint -c .eslintrc src/**/*.js",
    "lint": "npm run concurrently \"npm:lint:js\" \"npm:lint:styles\"",
    "lint:fix": "npm run concurrently  \"npm:lint:js -- --fix\" \"npm:lint:styles -- --fix\"",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack serve --mode=development --config ./demo/webpack.config.js",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex"
  ],
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write",
      "stylelint  \"src/**.css\" --config .stylelintrc.json --fix"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": ">= 7.4.0 < 8"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.5",
    "@babel/plugin-syntax-jsx": "7.14.5",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/preset-env": "7.15.6",
    "@donkeyclip/motorcortex": "7.5.2",
    "@donkeyclip/motorcortex-player": "2.2.3",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "concurrently": "6.3.0",
    "css-loader": "6.3.0",
    "es6-promise": "4.2.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "exports-loader": "3.0.0",
    "husky": "7.0.2",
    "imports-loader": "3.0.0",
    "npx": "10.2.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.58.0",
    "rollup-plugin-terser": "7.0.2",
    "shelljs": "0.8.4",
    "webpack": "5.56.1",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.3.1",
    "whatwg-fetch": "3.6.2"
  },
  "dependencies": {
    "polymorph-js": "^1.0.2"
  }
}
